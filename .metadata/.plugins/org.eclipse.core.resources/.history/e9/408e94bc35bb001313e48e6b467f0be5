public class RepositoryFactory {

	public RepositoryFactory(){
		
	} 
	
	public Repository createRepostitory(String type){
		Repository r;
		switch (type){
		case "evenement":
			r = new Repository() {
			};
			;

		default: //throw new exception;
		}
		return r;
	}
}
